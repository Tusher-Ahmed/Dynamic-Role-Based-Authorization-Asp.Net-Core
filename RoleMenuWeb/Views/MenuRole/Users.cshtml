@model List<RoleMenuWeb.ViewModel.UserViewModel>

@{
   	ViewData["Title"] = "Users";
 }
    
    <h1>Users</h1>
    <table class="table">
    	<thead>
    		<tr>
  			<th>
  				User Name
  			</th>
  			<th>
   				Email
 			</th>
   			<th>
                Action
   			</th>
   		</tr>
  	</thead>
   	<tbody>
   		@foreach (var item in Model)
  		{
   			<tr>
  				<td>
  					@Html.DisplayFor(modelItem => item.UserName)
   				</td>
   				<td>
   					@Html.DisplayFor(modelItem => item.Email)
  				</td>
   				<td>
 					<a asp-action="EditUser" asp-controller="MenuRole" asp-route-id="@item.Id">Edit</a>
   				</td>
   			</tr>
   		}
   	</tbody>
   </table>